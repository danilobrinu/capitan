<h3><%= question.question_text %></h3>
<table class="table table-striped">
  <thead>
    <td></td>
    <% question.answer_options.split(",").each do |value| %>
      <td><%= value %></td>
    <% end %>
  </thead>
  <tbody>
    <% question.sprint_users.each do |sprint_user| %>
    <tr>
      <td><%= sprint_user.user.profile.name %></td>
      <%=simple_form_for :jedi do |f|%>
        <%= f.collection_radio_buttons(:options, question_options(question.answer_options), :first, :last) do |b| %>
          <td><%= b.label { b.radio_button } %></td>
        <%end%>
      <%end%>
    </tr>
    <%end%>
  </tbody>
</table>
<br>
